<h1> Welcome <%= @user.nil?  ? "Bookmark Manager" : @user.email %>  </h1>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL:   <a href=https://<%=link.url%>><%= link.url%><a/>
      Tags:  <%= link.tags.map(&:name).join(", ") %>
    </li>
  <% end %>
</ul>
